	"tokenColors": [
		{
			"name": "Comments",
			"scope": "comment",
			"settings": {
				"foreground": "#6b6b6b",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Constants",
			"scope": "constant",
			"settings": {
				"foreground": "#cfbe9a"
			}
		},
		{
			"name": "Constant Numeric",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#bb4541"
			}
		},
		{
			"name": "Escape characters",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#55a385"
			}
		},
		{
			"name": "Named entities",
			"scope": "entity.name",
			"settings": {
				"foreground": "#FAC29AE6"
			}
		},
		{
			"name": "Function names",
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#55a385"
			}
		},
		{
			"name": "Tags",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#d8c9c9e6",
				"fontStyle": "normal"
			}
		},
		{
			"name": "Static types",
			"scope": [
				"entity.name.type",
				"storage.type.cs"
			],
			"settings": {
				"foreground": "#FAC29AE6"
			}
		},
		{
			"name": "Tag attributes",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#cfbe9a",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Inherited classes",
			"scope": "entity.other.inherited-class",
			"settings": {
				"foreground": "#da9188"
			}
		},
		{
			"name": "CSS ID selectors",
			"scope": "entity.other.attribute-name.id",
			"settings": {
				"foreground": "#55a385"
			}
		},
		{
			"name": "Pseudo CSS",
			"scope": [
				"entity.other.attribute-name.pseudo-element",
				"entity.other.attribute-name.pseudo-class"
			],
			"settings": {
				"foreground": "#da9188"
			}
		},
		{
			"name": "Variable names",
			"scope": [
				"entity.name.variable",
				"variable"
			],
			"settings": {
				"foreground": "#d8c9c9e6"
			}
		},
		{
			"name": "Keywords",
