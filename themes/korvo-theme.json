				"fontStyle": "italic"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string.quoted",
				"string.template"
			],
			"settings": {
				"foreground": "#da9188" 
			}
		},
		{
			"name": "Regex",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#cfbe9a"
			}
		},
		{
			"name": "Markup link descriptions",
			"scope": "string.other.link",
			"settings": {
				"foreground": "#cfbe9a"
			}
		},
		{
			"name": "Provided (support) values",
			"scope": "support",
			"settings": {
				"foreground": "#E9C9A7E6"
			}
		},
		{
			"name": "Provided functions",
			"scope": "support.function",
			"settings": { 
