				"entity.name.section"
			],
			"settings": {
				"foreground": "#d8c9c9e6"
			}
		},
		{
			"name": "Markup bold",
			"scope": "markup.bold",
			"settings": {
				"foreground": "#918bce",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup italics",
			"scope": "markup.italic",
			"settings": {
				"foreground": "#55a385",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup code",
